{
  "name": "MERN_template",
  "version": "1.0.0",
  "description": "Fullstack MERN base application with JWT Authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "watch": "nodemon server/server.js",
    "server": "node server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "author": "Erik Hoversten",
  "license": "MIT",
  "dependencies": {
    "caniuse-lite": "^1.0.30001255",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
